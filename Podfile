# platform :ios, '9.0'



def testing_pods
    pod 'Quick'
    pod 'Nimble'
end

target 'LambdaUI' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for LambdaUI

  target 'LambdaUITests' do
    inherit! :search_paths
    # Pods for testing
    testing_pods
  end

end

